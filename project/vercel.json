{
  "installCommand": "npm ci --include=dev",
  "buildCommand": "npm run build",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/server/index.vercel.js"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/index.html"
    }
  ],
  "outputDirectory": "dist",
  "framework": "vite"
}